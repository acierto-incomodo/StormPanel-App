{
  "name": "stormpanel-app",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win",
    "publish": "electron-builder --win --publish always"
  },
  "build": {
    "appId": "com.stormgamesstudios.stormpanelapp",
    "productName": "StormPanel App",
    "win": {
      "target": "nsis",
      "publish": [
        {
          "provider": "github",
          "owner": "acierto-incomodo",
          "repo": "StormPanel-App"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acierto-incomodo/electron-updater-example.git"
  },
  "keywords": [],
  "author": "StormGamesStudios",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/acierto-incomodo/electron-updater-example/issues"
  },
  "homepage": "https://github.com/acierto-incomodo/electron-updater-example#readme",
  "description": "",
  "dependencies": {
    "electron-updater": "^6.3.9"
  },
  "devDependencies": {
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8"
  }
}
